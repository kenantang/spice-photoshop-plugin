{
  "id": "c09047be",
  "name": "SPICE",
  "version": "0.0.3",
  "main": "index.html",
  "host": { "app": "PS", "minVersion": "27.2.0" },
  "manifestVersion": 5,
  "requiredPermissions": { "network": { "domains": "all" } },
  "entrypoints": [
    { "type": "command", "id": "showAlert", "label": "Show alert" },
    {
      "type": "panel",
      "id": "vanilla",
      "label": "SPICE",
      "minimumSize": { "width": 300, "height": 350 },
      "maximumSize": { "width": 2000, "height": 2000 },
      "preferredDockedSize": { "width": 300, "height": 500 },
      "icons": [
        { "width": 32, "height": 32, "path": "icons/dark.png", "scale": [1, 2], "theme": ["dark", "darkest"] },
        { "width": 32, "height": 32, "path": "icons/light.png", "scale": [1, 2], "theme": ["lightest", "light"] }
      ]
    },
    {
      "type": "panel",
      "id": "settings",
      "label": "SPICE Settings",
      "minimumSize": { "width": 300, "height": 450 },
      "maximumSize": { "width": 800, "height": 1000 },
      "preferredDockedSize": { "width": 300, "height": 600 },
      "icons": [
        { "width": 32, "height": 32, "path": "icons/dark.png", "scale": [1, 2], "theme": ["dark", "darkest"] },
        { "width": 32, "height": 32, "path": "icons/light.png", "scale": [1, 2], "theme": ["lightest", "light"] }
      ]
    }
  ],
  "icons": [
    { "width": 23, "height": 23, "path": "icons/dark.png", "scale": [1, 2], "theme": ["darkest", "dark", "medium"] },
    { "width": 23, "height": 23, "path": "icons/light.png", "scale": [1, 2], "theme": ["lightest", "light"] }
  ]
}